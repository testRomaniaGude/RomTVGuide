<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon
    id="script.SuperROTV"
	version="1.0.1"
	name="SuperTV"
	provider-name="King Leo">

	<requires>
		<import addon="xbmc.python" version="2.0"/>
	</requires>

	<extension point="xbmc.python.script"
        defaultresolution="720p"
        defaultresolutionwide="720p"
		library="default.py"/>

	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<minversion>35647</minversion>
		<summary lang="en">Watch Romanian LiveTV on your XBMC</summary>
		<description lang="en">SuperROTV enables you to watch TV streams from Romania, with test EPG.</description>
	</extension>
</addon>
